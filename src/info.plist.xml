				<string>function word() {
	 echo $1 | grep -i -q -x '[a-z]*'
}

vars=($1)
if word ${vars[0]}; then
    CHAIN_ID="${vars[1]}"
    if [ -z "$CHAIN_ID" ]; then
	    CHAIN_ID="0"
	fi
	./search.sh "$CHAIN_ID" ${vars[0]}
else 
    CHAIN_ID="${vars[0]}"
	if [ -z "$CHAIN_ID" ]; then 
	    CHAIN_ID="0"
	fi    
	./search.sh $CHAIN_ID ${vars[1]}
fi</string>
